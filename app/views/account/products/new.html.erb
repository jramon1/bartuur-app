
<div class="container-fluid">
  <div class="row">
    <div class="col-md-4 col-md-offset-4">
      <h1>Create Product</h1>
      <p>Hello, gogogo create your first product</p>

      <%=  simple_form_for [:account, @product] do |f| %>
        <%= f.input :name %>
        <%= f.input :description %>
        <%= f.input :photos, as: :attachinary %>
        <%= f.submit %>
      <% end %>
    </div>
  </div>
</div>
