 <%=  simple_form_for [:account, product] do |f| %>
      <div class="form-inputs">
        <%= f.input :photos, as: :attachinary %>
        <%= f.input :name %>
        <%= f.input :description, as: :text, input_html: {rows: 5, cols: 10} %>
        <%= f.input :value, as: :integer %>
      </div>

        <div class="form-actions">
          <%= f.submit %>
        </div>

      <% end %>
